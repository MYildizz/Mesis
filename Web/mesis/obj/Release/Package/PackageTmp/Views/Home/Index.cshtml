        //Sayacı veritabanina gönder
       function SayaciGonder(hours,minutes,seconds) {

            $.ajax({

                url: '/Home/SayacDb',
                type: 'POST',
                dataType: 'json',
                data: { saat: hours, dakika : minutes , saniye: seconds},
                error: function (data) {
                    alert('Oh no :(');
                }
            });
        }

    </script>


