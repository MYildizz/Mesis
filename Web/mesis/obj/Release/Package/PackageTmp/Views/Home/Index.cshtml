        $(document).ready(function () {
            $.getJSON(site1).done(function (veri) {

                var i;
                for (i = 0; i < 8; i++) {
                    SicaklikDeger[i] = veri[i].value;
                    temp = veri[i].date;
                    SicaklikZaman[i] = temp.slice(11, 16);

                }

                //Sıcaklık Grafiği
                new Morris.Line({
                    element: 'myfirstchart',
                    data: [

                        { year: SicaklikZaman[7], value: SicaklikDeger[7] },
                        { year: SicaklikZaman[6], value: SicaklikDeger[6] },
                        { year: SicaklikZaman[5], value: SicaklikDeger[5] },
                        { year: SicaklikZaman[4], value: SicaklikDeger[4] },
                        { year: SicaklikZaman[3], value: SicaklikDeger[3] },
                        { year: SicaklikZaman[2], value: SicaklikDeger[2] },
                        { year: SicaklikZaman[1], value: SicaklikDeger[1] },
                        { year: SicaklikZaman[0], value: SicaklikDeger[0] },
                    ],
                    xkey: 'year',
                    parseTime: false,
                    ykeys: ['value'],
                    labels: ['Value']
                });
            })
        })

        function SicaklikAl() {

            $(document).ready(function () {
                $.getJSON(site1).done(function (veri) {

                    var i;
                    for (i = 0; i < 5; i++) {
                        SicaklikDeger[i] = veri[i].value;
                        temp = veri[i].date;
                        SicaklikZaman[i] = temp.slice(11, 16);

                    }

                })
            })


        }

        // SicaklikAl();
        window.setInterval(SicaklikAl, 5000);


        //Nem Alma İşlemleri

        var site2 = "https://api.asksensors.com/read/Y4tLWrhgwCtpIgHHQvsJgT9cIebkCBhb?module=module2&10=number_of_entiries";

        //Jsondan veri alınıyor
        $(document).ready(function () {
            $.getJSON(site2).done(function (veri) {

                var i;
                for (i = 0; i < 8; i++) {
                    NemDeger[i] = veri[i].value;
                    temp = veri[i].date;
                    NemZaman[i] = temp.slice(11, 16);

                }
                //Nem Grafiği
                new Morris.Line({
                    element: 'mysecondchart',

                    data: [

                        { year: NemZaman[7], value: NemDeger[4] },
                        { year: NemZaman[6], value: NemDeger[4] },
                        { year: NemZaman[5], value: NemDeger[4] },
                        { year: NemZaman[4], value: NemDeger[4] },
                        { year: NemZaman[3], value: NemDeger[3] },
                        { year: NemZaman[2], value: NemDeger[2] },
                        { year: NemZaman[1], value: NemDeger[1] },
                        { year: NemZaman[0], value: NemDeger[0] },
                    ],
                    xkey: 'year',
                    parseTime: false,
                    ykeys: ['value'],
                    labels: ['Value']
                });
            })
        })

        function NemAl() {

            $(document).ready(function () {
                $.getJSON(site2).done(function (veri) {

                    var i;
                    for (i = 0; i < 5; i++) {
                        NemDeger[i] = veri[i].value;
                        temp = veri[i].date;
                        NemZaman[i] = temp.slice(11, 16);

                    }
                })
            })
        }
        NemAl();
        window.setInterval(NemAl, 1000);

        // Deger Alma işlemleri

        //Jsondan cihaz durumu verisi alınıyor
        var site3 = "https://api.asksensors.com/read/Y4tLWrhgwCtpIgHHQvsJgT9cIebkCBhb?module=module3&10=number_of_entiries";
        function degerAl() {

            $(document).ready(function () {
                $.getJSON(site3).done(function (veri) {
                    window.kapali = veri[0].value;
                    cihaz = veri[0].value;
                })
            })

            //Bağlantı yoksa
            if (cihaz == 3) {
                $("#CihazBaglanti").text("Cihaz Bağlantısı Yok");
                $(".CihazDurumu").css("background-color", "#57403A");
                $("#CihazBaglanti").css("font-size", "20px");
                $("#CihazBaglanti").css("top", "30px");

                if (hours < 10 && minutes < 10)
                    record = "0" + hours + "." + "0" + minutes + "." + second + " Dk";
                else if (hours > 10 && minutes < 10)
                    record = hours + "." + "0" + minutes + "." + second + " Dk";
                else if (hours < 10 && minutes > 10)
                    record = "0" + hours + "." + minutes + "." + second + " Dk"
                else
                    recod = hours + "." + minutes + "." + second + " Dk"

                date = d.getDate() + " " + d.getMonth() + " " + d.getFullYear();
            }

            //Cihaz kapalıysa
            else if (cihaz == 0) {

               // $(".CihazDurumu").animate({ background-color:"#6286a0"},'slow');

                $("#CihazBaglanti").text("Cihaz Kapalı");
               $(".CihazDurumu").css("background-color", "#6286a0");
                $("#CihazBaglanti").css("font-size", "20px");
                $("#CihazBaglanti").css("top", "30px");

                if (hours < 10 && minutes < 10)
                    record = "0" + hours + "." + "0" + minutes + "." + second + " Dk";
                else if (hours >= 10 && minutes < 10)
                    record = hours + "." + "0" + minutes + "." + second + " Dk";
                else if (hours < 10 && minutes > 10)
                    record = "0" + hours + "." + minutes + "." + second + " Dk"
                else
                    record = hours + "." + minutes + "." + second + " Dk"

                if ( second >= 10) {

                   //  $(".CihazDurumu").fadeTo("slow", 1);

                    var SicaklikNem = "" + SicaklikDeger[0] + "°C  -  " + NemDeger[0] + "%";
                    veriGonder(record, SicaklikNem);

                }


                minutes = 0;
                hours = 0;
                second = 0;
            }
                //Cihaz Açıksa
            else if (cihaz == 1) {

                $("#CihazBaglanti").text("Cihaz Açık");
                $(".CihazDurumu").css("background-color", "#831B00");
                $("#CihazBaglanti").css("font-size", "25px");
                $("#CihazBaglanti").css("top", "10px");
            }
        }
        degerAl();
        window.setInterval(degerAl, 1000);


        var bayrak;
        //Sayaç
        function sayac() {
            //Jsondan veri al
            function degerAl() {
                $(document).ready(function () {
                    $.getJSON(site3).done(function (veri) {
                        window.kapali = veri[0].value;
                        cihaz = veri[0].value;
                    })
                })
            }
            degerAl();
            window.setInterval(degerAl, 1000);

            //veritabanından gelen değer
            minutes = @Model.status.minutes;
            hours = @Model.status.hours;
            second = @Model.status.seconds;
            //Timer işlemleri
            function update() {



                if (cihaz == 1) {

                 if (hours < 10 && minutes < 10)
                    $("#time").text("0" + hours + "." + "0" + minutes + "." + second + " Dk")
                else if (hours > 10 && minutes < 10)
                    $("#time").text(hours + "." + "0" + minutes + "." + second + " Dk")
                else if (hours < 10 && minutes > 10)
                    $("#time").text("0" + hours + "." + minutes + "." + second + " Dk")
                else
                    $("#time").text(hours + "." + minutes + "." + second + " Dk")
                }



               if (cihaz == 0) {
                    $("#time").text("");
                    clearInterval(sayac);
                    //Döngüyü durdur
                }


                second += 1;
                if (second == 60) {
                    minutes += 1;
                    second = 0;
                }
                if (minutes === 60) {
                    hours += 1;
                    minutes = 0;
                }

                SayaciGonder(hours, minutes, second);




            }
            update();
            setInterval(update, 1000);
            bayrak = 1;

            if (cihaz == 0) {
                sayac();
                setInterval(sayac, 1000);
                bayrak = 0;

            }



        }
        sayac();
        // veritabanına bilgileri gönder
        function veriGonder(CihazDurumu,SicaklikNem) {

            $.ajax({

